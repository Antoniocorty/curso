package e03_visibilidad;

public class Inicio {

	public static void main(String[] args) {
		Rectangulo r = new Rectangulo(100, 50);
		
		r.setAlto(80);
		r.setAncho(40);
		
		System.out.println("El perímetro es:" + r.calcularPerimetro() );
		System.out.println("El área es:" + r.calcularArea() );
		

	}

}
